<h1>New Reviews And Rating</h1>
<p id="notice"><%= notice %></p>
<tbody>
<% if logged_in? %>
  <li><%= link_to "Logout", logout_path, method: "delete" %></li>

<% else %>
  <br>Please login to rate </br>
  <li><%= link_to "Login", login_path %></li>
<% end %>  

<% if logged_in? %>
  <%= render 'form' %>
<% end %>  
</tbody>